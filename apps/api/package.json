{
  "name": "@nodeweaver/api",
  "version": "1.0.4",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec tsx src/index.ts",
    "build": "tsc --project ./tsconfig.json",
    "start": "node dist/index.js",
    "db:migrate": "dotenv -e ../../.env -- prisma migrate dev",
    "db:seed": "dotenv -e ../../.env -- prisma db seed",
    "db:studio": "dotenv -e ../../.env -- prisma studio",
    "db:push": "dotenv -e ../../.env -- prisma db push",
    "generate:key": "tsx src/scripts/generate-master-key.ts",
    "rotate:key": "tsx src/scripts/rotate-master-key.ts",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pdfkit": "^0.17.4",
    "axios": "^1.6.2",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.1.5",
    "cors": "^2.8.5",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^8.1.0",
    "https-proxy-agent": "^5.0.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.3",
    "leaflet": "^1.9.4",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "pdfkit": "^0.17.2",
    "react-leaflet": "^5.0.0",
    "socket.io": "^4.6.1",
    "socks-proxy-agent": "^8.0.5",
    "telegraf": "^4.16.3",
    "undici": "^7.19.2",
    "uuid": "^9.0.1",
    "xml2js": "^0.6.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@types/xml2js": "^0.4.14",
    "dotenv-cli": "^11.0.0",
    "nodemon": "^3.1.11",
    "prisma": "5.22.0",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3"
  }
}
